if (!gameover) {
    score_str = "Score: " + string(score);
    life_str = "Health: " + string(obj_circle.health);
    ammo_str = "Ammo: " + string(obj_gun.num_bullets);
}

if (!gameover and obj_circle.health &lt;= 0) {
    gameover = true;
    global.SPD = 0;
    alarm[0] = room_speed * 1;
    with (obj_circle) {
        instance_destroy();
    }
    with (obj_gun) {
        instance_destroy();
    }
    
    //Set high score
    ini_open("Score.ini");
    hiscore = ini_read_real("Score", "0", 0);
    hiscore_new = false;
    if (score &gt; hiscore) {
        ini_write_real("Score","0",score);
        hiscore_new = true;
        hiscore = score;
    }
    ini_close();
}

if (gameover and !delay and keyboard_check_pressed(vk_anykey)) {
    room_restart();
}
