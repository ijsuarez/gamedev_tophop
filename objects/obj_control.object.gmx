            <string>if (winx != window_get_x() or winy != window_get_y() or
    os_is_paused() or !window_has_focus() or delta_time &gt; 100000) {
    winx = window_get_x();
    winy = window_get_y();
    global.SPD = 0;
} else if (!global.GAMEOVER) {
    global.SPD = 100;
}

global.TIME_PASSED = global.SPD * delta_time / 1000000;

if (!global.GAMEOVER) {
    score_str = "Score: " + string(score);
    //life_str = "Health: " + string(obj_circle.health);
    //ammo_str = "Ammo: " + string(obj_gun.num_bullets);
}

if (!global.GAMEOVER and obj_circle.health &lt;= 0) {
    global.GAMEOVER = true;
    global.SPD = 0;
    alarm[0] = room_speed * 1;
    with (obj_circle) {
        instance_destroy();
    }
    with (obj_gun) {
        instance_destroy();
    }
    
    //Set high score
    ini_open("Score.ini");
    hiscore = ini_read_real("Score", "0", 0);
    hiscore_new = false;
    if (score &gt; hiscore) {
        ini_write_real("Score","0",score);
        hiscore_new = true;
        hiscore = score;
    }
    ini_close();
}

if (global.GAMEOVER and hiscore_new) {
    part_emitter_burst(global.PSYSTEM,emitter_win,particle_win_r,1);
    part_emitter_burst(global.PSYSTEM,emitter_win,particle_win_b,1);
    part_emitter_burst(global.PSYSTEM,emitter_win,particle_win_g,1);
}

if (global.GAMEOVER and !delay and keyboard_check_pressed(vk_space)) {
    part_particles_clear(global.PSYSTEM);
    room_restart();
}
